{
    "format_version": "1.19.0",
    "minecraft:block": {
        "description": {
            "identifier": "trebesin:imageboard_ground_czech_emblem",
            "properties": {
				"trebesin:horizontal_rotation": [2, 3, 4, 5, 6],
				"trebesin:vertical_rotation": [1 ,0]
			},
            "is_experimental": true,
            "register_to_creative_menu": true,
            "menu_category": {
                "category": "construction"
            }
        },
        "components": {
            "minecraft:geometry": "geometry.imageboard_ground",
            "minecraft:destroy_time": 0.0,
            "minecraft:block_light_filter": 0,
            "minecraft:collision_box": {
                "origin": [-8,0,-8],
                "size": [16,1,16]
            },
            "minecraft:selection_box": {
                "origin": [-8,0,-8],
                "size": [16,1,16]
            },
            "minecraft:material_instances": {
                "up": {
                    "texture": "image_czech_emblem",
                    "render_method": "alpha_test"
                },
                "down": {
                    "texture": "image_czech_emblem",
                    "render_method": "alpha_test"
                },
                "*": {
                    "texture": "wood_oak",
                    "render_method": "alpha_test"
                }
            },
            "minecraft:on_player_placing": {
                "event": "trebesin:set_rotation",
                "target": "self"
            },
            "minecraft:creative_category": {
				"category": "construction"
			}
        },
        "events": {
            "trebesin:set_rotation": {
                "set_block_property": {
                    "trebesin:horizontal_rotation": "query.cardinal_facing_2d",
					"trebesin:vertical_rotation": "query.block_face"
                }
            }
        },
        "permutations": [
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 2.0 && query.block_property('trebesin:vertical_rotation') == 1.0",
                "components": {
                    "minecraft:rotation": [
                        0,
                        0,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 3.0 && query.block_property('trebesin:vertical_rotation') == 1.0",
                "components": {
                    "minecraft:rotation": [
                        0,
                        180,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 4.0 && query.block_property('trebesin:vertical_rotation') == 1.0",
                "components": {
                    "minecraft:rotation": [
                        0,
                        90,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 5.0 && query.block_property('trebesin:vertical_rotation') == 1.0",
                "components": {
                    "minecraft:rotation": [
                        0,
                        270,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 6.0 && query.block_property('trebesin:vertical_rotation') == 1.0",
                "components": {
                    "minecraft:rotation": [
                        0,
                        0,
                        0
                    ]
                }
            },
		    {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 2.0 && query.block_property('trebesin:vertical_rotation') == 0.0",
                "components": {
                    "minecraft:rotation": [
                        180,
                        180,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 3.0 && query.block_property('trebesin:vertical_rotation') == 0.0",
                "components": {
                    "minecraft:rotation": [
                        180,
                        0,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 4.0 && query.block_property('trebesin:vertical_rotation') == 0.0",
                "components": {
                    "minecraft:rotation": [
                        180,
                        270,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 5.0 && query.block_property('trebesin:vertical_rotation') == 0.0",
                "components": {
                    "minecraft:rotation": [
                        180,
                        90,
                        0
                    ]
                }
            },
            {
                "condition": "query.block_property('trebesin:horizontal_rotation') == 6.0 && query.block_property('trebesin:vertical_rotation') == 0.0",
                "components": {
                    "minecraft:rotation": [
                        180,
                        180,
                        0
                    ]
                }
            }
        ]
    }
}